<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="HTMLPalette">
        <folder name="Bootstrap UI">
            <attr name="position" intvalue="1"/>
            <file name="blankPage.xml" url="items/resources/blankPage.xml">
                <attr name="position" intvalue="100"/>
            </file>
        </folder>
        <folder name="Bootstrap Grid">
            <attr name="position" intvalue="2"/>
            <file name="gridRow.xml" url="items/resources/gridRow.xml">
                <attr name="position" intvalue="100"/>
            </file>
            <file name="gridCol.xml" url="items/resources/gridCol.xml">
                <attr name="position" intvalue="200"/>
            </file>
            <file name="gridClearFix.xml" url="items/resources/gridClearFix.xml">
                <attr name="position" intvalue="300"/>
            </file>
        </folder>
        <folder name="Bootstrap Typography">
            <attr name="position" intvalue="3"/>
            <file name="heading.xml" url="items/resources/heading.xml">
                <attr name="position" intvalue="100"/>
            </file>
            <file name="paragraph.xml" url="items/resources/paragraph.xml">
                <attr name="position" intvalue="200"/>
            </file>
            <file name="alert.xml" url="items/resources/alert.xml">
                <attr name="position" intvalue="300"/>
            </file>
            <file name="blockquote.xml" url="items/resources/blockquote.xml">
                <attr name="position" intvalue="400"/>
            </file>
            <file name="type-more.xml" url="items/resources/type-more.xml">
                <attr name="position" intvalue="500"/>
            </file>
        </folder>
        <folder name="Bootstrap Images">
            <attr name="position" intvalue="4"/>
            <file name="image.xml" url="items/resources/image.xml">
                <attr name="position" intvalue="100"/>
            </file>
            <file name="glyphicons.xml" url="items/resources/glyphicons.xml">
                <attr name="position" intvalue="200"/>
            </file>
        </folder>
    </folder>
</filesystem>
